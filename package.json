{
  "name": "auth-user-management-starter",
  "version": "1.0.0",
  "description": "A comprehensive authentication and user management system with Node.js, Express, and MySQL",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "start": "node src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:unit:watch": "jest --selectProjects unit --watch",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "lint:fix": "eslint src/**/*.js tests/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\" \"scripts/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\" \"scripts/**/*.js\"",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:seed:prod": "NODE_ENV=production node prisma/seed.js",
    "db:reset": "npx prisma migrate reset --force",
    "db:test:setup": "NODE_ENV=test npx prisma migrate deploy",
    "db:test:reset": "NODE_ENV=test npx prisma migrate reset --force",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:clean": "docker compose down -v --remove-orphans",
    "docker:rebuild": "docker compose down && docker compose build --no-cache && docker compose up -d",
    "setup": "node scripts/setup.js",
    "setup:dev": "node scripts/setup-dev.js",
    "setup:prod": "node scripts/setup-prod.js",
    "build": "node scripts/build.js",
    "deploy": "node scripts/deploy.js",
    "health": "node scripts/health-check.js",
    "backup:db": "node scripts/backup-db.js",
    "restore:db": "node scripts/restore-db.js",
    "prepare": "husky install",
    "postinstall": "npm run db:generate"
  },
  "keywords": [
    "authentication",
    "user-management",
    "nodejs",
    "express",
    "mysql",
    "jwt",
    "oauth",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "mysql2": "^3.6.5",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}